{
  "name": "opnet-callable",
  "version": "0.1.0",
  "description": "callable AssemblyScript interfaces for OP_NET",
  "author": "flex",
  "main": "lib/index.js",
  "type": "module",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "assembly": {
      "import": "./assembly/index.ts"
    }
  },
  "scripts": {
    "build:transform": "tsc",
    "build": "npm-run-all build:transform asbuild:untouched",
    "build:test": "asc --transform . test/index.ts",
    "asbuild:untouched": "asc assembly/index.ts --target debug",
    "asbuild:optimized": "asc assembly/index.ts --target release",
    "asbuild": "yarn asbuild:untouched && yarn asbuild:optimized",
    "prettier:ts": "prettier --write src.ts/**/*.ts",
    "prettier:as": "prettier --write assembly/**/*.ts"
  },
  "devDependencies": {
    "assemblyscript": "^0.27.29",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/node": "^18.11.13",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@btc-vision/btc-runtime": "^1.1.3",
    "lodash.clonedeep": "^4.5.0",
    "ts-mixer": "^6.0.2",
    "visitor-as": "0.7.1-0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kungfuflex/op-callable.git"
  },
  "keywords": [
    "assemblyscript",
    "callable",
    "OP_NET"
  ]
}
